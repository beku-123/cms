#include<stdio.h>
main()
{
	int i,n,m,t,k,w,max,q1,q2,w1,a,s1[1000000],s2[1000000];
	char a1[2];
	scanf("%d%d",&n,&m);
	t=0; k=0; w=0;
	for(i=0; i<n; i++) { scanf("%d",&a); scanf("%s",a1); if(a1[0]=='L') {while(a) {s1[t]=k; t++; k--; a--;}} else { while(a) {s1[t]=k; t++; k++; a--;}}}
	s1[t]=k; q1=k; w=t; t=0; k=0; 
	for(i=0; i<m; i++) { scanf("%d",&a); scanf("%s",a1); if(a1[0]=='L') {while(a) {s2[t]=k; t++; k--; a--;}} else { while(a) {s2[t]=k; t++; k++; a--;}}}
	s2[t]=k; q2=k; w1=t; 
	if(w<w1) {max=w1; for(i=w+1; i<=w1; i++) s1[i]=q1;} else {max=w; for(i=w1+1; i<=w; i++) s2[i]=q2;}
	t=0;	
	for(i=2; i<=max; i++) {
		if(s1[i-1]!=s2[i-1]&&s1[i]==s2[i]) t++;
	} 
	printf("%d",t);
}
